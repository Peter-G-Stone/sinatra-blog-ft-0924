<h1> Create a new Post</h1>
<form method="POST" action="/posts">
    <select name="user_id">
        <% @users.each do |user| %>
            <option value="<%= user.id %>"><%= user.name %></option>
        <% end %>
    </select>
    <input type="text" name="title">
    <textarea  name="body" ></textarea>
    <input type="submit">
</form>